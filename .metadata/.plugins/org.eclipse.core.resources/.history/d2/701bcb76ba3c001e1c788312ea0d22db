package operator;

public class Array {

	public static void main(String[] args) {
		int a;
		int c[]=new int[5];
		int b[]= {1,2,3,4,5,6};
		a=9;
		c[0]=10;
		
	System.out.println(a);
	System.out.println(b[0]);
	for(int i=0;i<b.length;i++) {
		System.out.println(b[i]*2);
	}
	System.out.println(b.length);
	

}}
