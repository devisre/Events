package operator;

public class Array_2Deg2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [][]a= {{1,2},{3,4}};
		int [][]b= {{1,1},{1,1}};
		for(int i=0;i<a.length;i++) {
			for(int j=0;j<a[i].length;j++) {
				System.out.println(a[i][j]);
			}
		}
		

	}

}
