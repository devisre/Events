package operator;

public class while_ex2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int a=1;
 while (a<=10) {
	 if (a%3==0);{
		 System.out.println("multiple of 3");}
		 if (a%5==0);{
			 System.out.println("multiple of 5");}
		 
	
	System.out.println(a);
	a++;
}

	}

}
