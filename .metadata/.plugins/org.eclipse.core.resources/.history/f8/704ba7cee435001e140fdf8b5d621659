package operator;

public class switch_example {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number=30;
		switch (number) {
		case 10->System.out.println("monday");
		case 30->System.out.println(number);
	
		default->System.out.println("error");
		
		}
		
		String name="devipriya";
		switch (name) {
		case "priya"->System.out.println(name);
		case "sreepriya"->System.out.println(name);
		default->System.out.println("error");}
	}

}
