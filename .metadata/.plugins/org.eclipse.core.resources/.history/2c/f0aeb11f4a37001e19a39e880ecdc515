package operator;

import java.util.Scanner;

public class forloopfactorial {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);
int a=sc.nextInt();
int c=0;
for ( int b=1;b<=a;b++) {
	c=b*(b+1);
	System.out.println(c);
	
}
//System.out.println(c);
	}

}
