package operator;

public class nestedfor1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//int a=1;
		for (int i=1;i<=3;i++) {
			System.out.print("*");
			for (int a=1;a<=1;a++) 
		System.out.println("*");
			for (int b=1;b<=2;b++)
				System.out.print("*");
		
			
	
				//System.out.println("");}
				
		}}


	}
